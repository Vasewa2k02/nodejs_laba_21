<div class="contacts">
   {{#each contacts}}
        {{>contact}}
    {{/each}}
</div>
<div class="add-form">
    <input class="name" type="text"placeholder="Name">
    <input class="phone-number" type="text" placeholder="Phone number">
    <a class="default-link add-link" onclick="createContact()">Add</a>
    <a class="default-link refuse-link" onclick="{{refuse}}">Refuse</a>
</div>
