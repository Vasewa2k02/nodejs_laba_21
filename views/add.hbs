<div class="contacts">
   {{#each contacts}}
        {{>contact}}
    {{/each}}
</div>
<form class="add-form">
    <input class="name" type="text" placeholder="Name">
    <input class="phone-number" type="text" placeholder="Phone number">
    <input type="button" class="default-link add-link" onclick="createContact()" value="Add"/>
    <input type="button" class="default-link refuse-link" onclick="{{refuse}}" value="Refuse"/>
</form>
